#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sudilajayasekara/education/sliit/y3:s2:af/af-2-Sudila-Jayasekara/node_modules/.pnpm/acorn@8.14.1/node_modules/acorn/bin/node_modules:/Users/sudilajayasekara/education/sliit/y3:s2:af/af-2-Sudila-Jayasekara/node_modules/.pnpm/acorn@8.14.1/node_modules/acorn/node_modules:/Users/sudilajayasekara/education/sliit/y3:s2:af/af-2-Sudila-Jayasekara/node_modules/.pnpm/acorn@8.14.1/node_modules:/Users/sudilajayasekara/education/sliit/y3:s2:af/af-2-Sudila-Jayasekara/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sudilajayasekara/education/sliit/y3:s2:af/af-2-Sudila-Jayasekara/node_modules/.pnpm/acorn@8.14.1/node_modules/acorn/bin/node_modules:/Users/sudilajayasekara/education/sliit/y3:s2:af/af-2-Sudila-Jayasekara/node_modules/.pnpm/acorn@8.14.1/node_modules/acorn/node_modules:/Users/sudilajayasekara/education/sliit/y3:s2:af/af-2-Sudila-Jayasekara/node_modules/.pnpm/acorn@8.14.1/node_modules:/Users/sudilajayasekara/education/sliit/y3:s2:af/af-2-Sudila-Jayasekara/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
